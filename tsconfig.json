{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"lib": [
			"es6",
		],
		"sourceMap": true,
		"rootDir": "src",
		"alwaysStrict": true,
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}